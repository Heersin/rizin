NAME=ELF_ABI : gcc m32 O0 load
FILE=bins/abi_bins/elf/compilers/gcc/echo_gcc_m32_O0
CMDS=<<EOF
s
EOF
EXPECT=<<EOF
0x8048e40
EOF
RUN

NAME=ELF_ABI : gcc m32 O0 arch/bits/size ground truth
FILE=bins/abi_bins/elf/compilers/gcc/echo_gcc_m32_O0
CMDS=<<EOF
iI~arch,binsz,bits
EOF
EXPECT=<<EOF
arch     x86
binsz    110669
bits     32
EOF
RUN

NAME=ELF_ABI : gcc m32 O0 dwarf info
FILE=bins/abi_bins/elf/compilers/gcc/echo_gcc_m32_O0
CMDS=<<EOF
id~?
EOF
EXPECT=<<EOF
25559
EOF
RUN

NAME=ELF_ABI : gcc m32 O0 flagspaces
FILE=bins/abi_bins/elf/compilers/gcc/echo_gcc_m32_O0
ARG=-A
CMDS=<<EOF
fs
EOF
EXPECT=<<EOF
    0 * classes
   45 * imports
   47 * relocs
   39 * sections
   10 * segments
   68 * strings
  137 * symbols
   35 * symbols.sections
EOF
RUN

